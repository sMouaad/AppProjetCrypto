<div id="<%= dom_id medical_record %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Title:</strong>
    <%= medical_record.title %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Content:</strong>
    <%= medical_record.content %>
  </div>
  <div>
    <strong class="block font-medium mb-1">User:</strong>
    <%= medical_record.user_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Abe attributes:</strong>
    <%= medical_record.abe_attributes %>
  </div>
</div>
